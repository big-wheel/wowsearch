{
  "name": "wowsearch",
  "version": "1.0.0",
  "main": "index.js",
  "description": "The search's workflow like docsearch.",
  "author": "big-wheel",
  "scripts": {
    "bootstrap": "PUPPETEER_DOWNLOAD_HOST=https://storage.googleapis.com.cnpmjs.org lerna bootstrap",
    "test": "jest",
    "dev": "lerna run dev --parallel",
    "prepublishOnly": "npm test"
  },
  "keywords": [
    "wowsearch"
  ],
  "license": "MIT",
  "repository": "big-wheel/wowsearch",
  "edam": {
    "source": "./packages/__template",
    "output": "./packages"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testMatch": [
      "**/__test{s,}__/*.(spec|test).{t,j}s{x,}"
    ]
  },
  "devDependencies": {
    "@types/jest": "^23.3.14",
    "@types/node": "^10.3.2",
    "jest": "^23.6.0",
    "lerna": "^2.7.1",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "ts-jest": "^22.4.6",
    "typescript": "^3.6.3"
  }
}
